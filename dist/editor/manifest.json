{
    "name": "ZahoriJs",
    "description": "ZahoriJs",
    "version": "1.2",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "browser_action": {
        "default_title": "ZahoriJs",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon2.png",
        "48": "icon3.png",
        "128": "icon4.png"
    },
    "background": {
        "scripts": ["main.js"]
    },
    "manifest_version": 2
}
